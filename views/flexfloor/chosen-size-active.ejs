<style>

</style>

<a-text position="0 3 -10" scale="3 3 3" align="center" value="FlexFloor Room <%= size %>' X <%= size %>'"></a-text>
<a-text color="red" position="0 2 -10" scale="2 2 2" align="center" value="FlexFloor Room <%= sizeMeters %>m X <%= sizeMeters %>m"></a-text>

<a-entity id="flexRoom" opacity="0.5"> 
    <a-box id="floor" opacity="1   " position="0 0 0" scale="<%= sizeMeters %> 0.1 <%= sizeMeters %>" color="white" rotation="0 0 0" material="shader: flat; src: #oak1_Img; repeat: 1 1; metalness: 0.25; roughnessMap: #oak1_Rough; roughness: 0.8; roughnessTextureRepeat: 1 1; envMap: #oak1_Height;"></a-box>
    <a-box id="ceiling" opacity="1   " position="0 3 0" scale="<%= sizeMeters %> 0.1 <%= sizeMeters %>" color="white" rotation="0 0 0" material="shader: flat; src: #oak1_Img; repeat: 1 1; metalness: 0.25; roughnessMap: #oak1_Rough; roughness: 0.8; roughnessTextureRepeat: 1 1; envMap: #oak1_Height;"></a-box>
    <a-box id="wall1" opacity="1   " position="<%= wallPosition %> 1.5 0" scale="<%= sizeMeters %> 0.1 3" color="white" rotation="90 90 0" material="src: #log1_Img; repeat: 1 1; normalMap: #log1_Norm; normalTextureRepeat: 1 1; metalness: 0.25; normalScale: 2 2; normalTextureOffset: 2 2; displacementMap: #log1_Dis; ambientOcclusionMap: #pat1_Occ; ambientOcclusionTextureRepeat: 1 1; roughnessMap: #log1_Rough; roughness: 0.8; roughnessTextureRepeat: 1 1; envMap: #log1_Height;"></a-box>
    <a-box id="wall2" opacity="1   " position="-<%= wallPosition %> 1.5 0" scale="<%= sizeMeters %> 0.1 3" color="white" rotation="90 90 0" material="src: #log1_Img; repeat: 1 1; normalMap: #log1_Norm; normalTextureRepeat: 1 1; metalness: 0.25; normalScale: 2 2; normalTextureOffset: 2 2; displacementMap: #log1_Dis; ambientOcclusionMap: #pat1_Occ; ambientOcclusionTextureRepeat: 1 1; roughnessMap: #log1_Rough; roughness: 0.8; roughnessTextureRepeat: 1 1; envMap: #log1_Height;"></a-box>
    <a-box id="wall3" opacity="1   " position="0 1.5 <%= wallPosition %>" scale="<%= sizeMeters %> 0.1 3" color="white" rotation="90 0 0" material="src: #log1_Img; repeat: 1 1; normalMap: #log1_Norm; normalTextureRepeat: 1 1; metalness: 0.25; normalScale: 2 2; normalTextureOffset: 2 2; displacementMap: #log1_Dis; ambientOcclusionMap: #pat1_Occ; ambientOcclusionTextureRepeat: 1 1; roughnessMap: #log1_Rough; roughness: 0.8; roughnessTextureRepeat: 1 1; envMap: #log1_Height;"></a-box>
    <a-box id="wall4" opacity="1   " position="0 1.5 -<%= wallPosition %>" scale="<%= sizeMeters %> 0.1 3" color="white" rotation="90 0 0" material="src: #log1_Img; repeat: 1 1; normalMap: #log1_Norm; normalTextureRepeat: 1 1; metalness: 0.25; normalScale: 2 2; normalTextureOffset: 2 2; displacementMap: #log1_Dis; ambientOcclusionMap: #pat1_Occ; ambientOcclusionTextureRepeat: 1 1; roughnessMap: #log1_Rough; roughness: 0.8; roughnessTextureRepeat: 1 1; envMap: #log1_Height;"></a-box>
</a-entity>

<a-light type="point" position="0 2.5 0" ></a-light>